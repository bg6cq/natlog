#!/bin/sh

#
# cat multirun.rc

iptables -F
iptables -t nat -F

iptables -t nat -A PREROUTING -j DNAT -d 218.104.71.166 -p tcp --to 202.38.64.200
iptables -t nat -A POSTROUTING -s 202.38.64.200 -j SNAT -o eth0 --to 218.104.71.166


#iptables -t nat -A POSTROUTING -j SNAT -o eth0 --to 218.104.71.162 -s 202.38.64.143
iptables -t nat -A POSTROUTING -j SNAT -o eth0 --to 218.104.71.166
#iptables -t nat -A POSTROUTING -j SNAT -o eth0 --to 218.104.71.162
# 218.104.71.162 blocked 2012-12-04

iptables -A FORWARD -j DROP -p udp --dport 135:139
iptables -A FORWARD -j DROP -p tcp --dport 135:139
iptables -A FORWARD -j DROP -p tcp --dport 445

iptables -A INPUT -j ACCEPT -p udp -s 202.38.64.1
iptables -A INPUT -j ACCEPT -p udp -s 202.38.64.7
iptables -A INPUT -j DROP -p udp

iptables -A INPUT -j ACCEPT -p tcp --dport 22 -s 202.38.64.0/24
iptables -A INPUT -j ACCEPT -p tcp --dport 22 -s 202.38.96.0/24
iptables -A INPUT -j DROP -p tcp --dport 22
